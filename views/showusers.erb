<h2>Your data</h2>



<table class="table">
 <thead>
		<tr>
			<th>Name</th>
			<th>Phone</th>
			<th>Date</th>
			<th>Barber</th>
			<th>Color</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |row|%>
		<tr>
			<td><%= row['username']%> </td>
			<td><%= row['phone']%></td>
			<td><%= row['datestamp']%></td>
			<td><%= row['barber']%></td>
			<td style="background-color: <%= row['color']%>"><%= row['color']%></td>
		</tr>
		<%end%>
	</tbody>

</table>
